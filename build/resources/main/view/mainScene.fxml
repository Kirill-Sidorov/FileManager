<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="600.0" prefWidth="1000.0" stylesheets="/style/styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sidorov.filemanager.controller.MainController">
    <MenuBar>
        <Menu>

        </Menu>
    </MenuBar>

    <HBox styleClass="workplace" VBox.vgrow="ALWAYS">
        <fx:include fx:id="rightFileTable" source="fileTable.fxml" HBox.hgrow="ALWAYS" />
        <VBox alignment="CENTER">
            <Button styleClass="button-right-copy"/>
            <Button styleClass="button-left-copy"/>
        </VBox>
        <fx:include fx:id="leftFileTable" source="fileTable.fxml" HBox.hgrow="ALWAYS" />
    </HBox>
</VBox>
